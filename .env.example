# server
MODX__SERVER__APPNAME=ModX Server
MODX__SERVER__DESCRIPTION=:)
MODX__SERVER__VERSION=1.0.0
MODX__SERVER__HTTP_HOST=0.0.0.0
MODX__SERVER__HTTP_PORT=8000
MODX__SERVER__ROUTE_PREFIX=/v1
MODX__SERVER__OPENAPI_ROUTE=/openapi
MODX__SERVER__DEBUG=false
MODX__SERVER__WORKERS=1
# MODX__SERVER__SSL_KEYFILE=
# MODX__SERVER__SSL_CERTFILE=
# MODX__SERVER__SSL_KEYFILE_PASSWORD=
# MODX__SERVER__SSL_VERSION=
# MODX__SERVER__SSL_CERT_REQS=
# MODX__SERVER__SSL_CA_CERTS=
# MODX__SERVER__SSL_CIPHERS=
# MODX__SERVER__HEADERS=
# MODX__SERVER__H11_MAX_INCOMPLETE_EVENT_SIZE=

# chatbot
MODX__CHATBOT__CACHE_TTL=218000

# logging
MODX__LOGGING__BACKEND=native
MODX__LOGGING__TARGETS="[{\"logname\": \"stdout\", \"loglevel\": \"info\"}]"

# middleware.auth
MODX__MIDDLEWARE__AUTH__UNPROTECTED_ROUTES=[]

# middleware.logging
MODX__MIDDLEWARE__LOGGING__COLORIZE=true
MODX__MIDDLEWARE__LOGGING__TRACE_ID_HEADER=X-Trace-Id
MODX__MIDDLEWARE__LOGGING__SPAN_ID_HEADER=X-Span-Id
MODX__MIDDLEWARE__LOGGING__PARENT_SPAN_ID_HEADER=X-Parent-Span-Id
MODX__MIDDLEWARE__LOGGING__LOG_HEADERS=true
MODX__MIDDLEWARE__LOGGING__LOG_QUERY_STRING=true
MODX__MIDDLEWARE__LOGGING__EXCLUDE_PATHS=[]

# middleware.security
MODX__MIDDLEWARE__SECURITY__ENABLED=true
MODX__MIDDLEWARE__SECURITY__ENFORCE_HTTPS=false
MODX__MIDDLEWARE__SECURITY__HSTS_MAX_AGE=31536000
MODX__MIDDLEWARE__SECURITY__ADD_CONTENT_TYPE_OPTIONS=true
MODX__MIDDLEWARE__SECURITY__CACHE_CONTROL="no-cache, no-store, must-revalidate"
MODX__MIDDLEWARE__SECURITY__ADD_REQUEST_ID=true
MODX__MIDDLEWARE__SECURITY__ADD_API_VERSION=true
MODX__MIDDLEWARE__SECURITY__API_VERSION=1.0.0

# middleware.trace
MODX__MIDDLEWARE__TRACE__ENABLED=false
MODX__MIDDLEWARE__TRACE__TRACE_ID_HEADER=X-Trace-Id
MODX__MIDDLEWARE__TRACE__SPAN_ID_HEADER=X-Span-Id
MODX__MIDDLEWARE__TRACE__PARENT_SPAN_ID_HEADER=X-Parent-Span-Id
MODX__MIDDLEWARE__TRACE__LOG_TRACE_INFO=true

# middleware.cors
MODX__MIDDLEWARE__CORS__ENABLED=true
MODX__MIDDLEWARE__CORS__ALLOW_ORIGINS=["*"]
MODX__MIDDLEWARE__CORS__ALLOW_CREDENTIALS=true
MODX__MIDDLEWARE__CORS__ALLOW_METHODS=["*"]
MODX__MIDDLEWARE__CORS__ALLOW_HEADERS=["*"]
MODX__MIDDLEWARE__CORS__ALLOW_ORIGIN_REGEX=
MODX__MIDDLEWARE__CORS__EXPOSE_HEADERS=[]
MODX__MIDDLEWARE__CORS__MAX_AGE=600

# middleware.gzip
MODX__MIDDLEWARE__GZIP__ENABLED=true
MODX__MIDDLEWARE__GZIP__MINIMUM_SIZE=1024
MODX__MIDDLEWARE__GZIP__COMPRESSLEVEL=9

# prometheus
MODX__PROMETHEUS__ENABLED=true
MODX__PROMETHEUS__METRICS_PATH=/metrics
MODX__PROMETHEUS__TRACK_IN_PROGRESS=true
# MODX__PROMETHEUS__BUCKETS=
MODX__PROMETHEUS__EXCLUDE_PATHS=[]
# MODX__PROMETHEUS__CUSTOM_LABELS=
MODX__PROMETHEUS__ENABLE_EXEMPLARS=false

# cache
MODX__CACHE__SPLIT=:
MODX__CACHE__PREF=modx:
MODX__CACHE__DEFAULT_TTL=3600
MODX__CACHE__NEGATIVE_TTL=60
MODX__CACHE__REDIS__HOST=localhost
MODX__CACHE__REDIS__PORT=6379
MODX__CACHE__REDIS__DB=0
MODX__CACHE__REDIS__PASSWORD=
MODX__CACHE__REDIS__SOCKET_TIMEOUT=5
MODX__CACHE__REDIS__SOCKET_CONNECT_TIMEOUT=5
MODX__CACHE__REDIS__RETRY_ON_TIMEOUT=false
MODX__CACHE__REDIS__MAX_CONNECTIONS=10
MODX__CACHE__REDIS__HEALTH_CHECK_INTERVAL=30
MODX__CACHE__REDIS__SSL=false
MODX__CACHE__REDIS__SSL_CERT_REQS=required
MODX__CACHE__REDIS__SECURE_SERIALIZATION=false

# http_client
MODX__HTTP_CLIENT__TIMEOUT=10
MODX__HTTP_CLIENT__MAX_CONNECTIONS=100
MODX__HTTP_CLIENT__MAX_KEEPALIVE_CONNECTIONS=20
MODX__HTTP_CLIENT__MAX_REDIRECTS=10
MODX__HTTP_CLIENT__HTTP2=true
MODX__HTTP_CLIENT__TRUST_ENV=true
MODX__HTTP_CLIENT__USER_AGENT=ModX-Client/1.0.0

# misc
MODX__KEYS_FILE=.keys
MODX__MODELS_FILE=.models.json
